<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Antlr4.Runtime</id>
        <version>0.0.0</version>
        <authors>ProphetLamb, Sam Harwell, Terence Parr</authors>
        <owners>ProphetLamb</owners>
        <description>The runtime library for parsers generated by the C# target of ANTLR 4. This package supports projects targeting .NET 2.0 or newer, and built using Visual
            Studio 2008 or newer.
        </description>
        <language>en-us</language>
        <projectUrl>https://github.com/ProphetLamb-Organistion/antlr4cs/antlr4cs</projectUrl>
        <license type="file">assets\LICENSE.txt</license>
        <icon>assets\antlr.png</icon>
        <copyright>Copyright Â© ProphetLamb 2021</copyright>
        <releaseNotes>https://github.com/ProphetLamb-Organistion/antlr4cs/releases/v$version$</releaseNotes>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <tags>antlr antlr4 parsing</tags>
        <title>ANTLR 4 Runtime</title>
        <summary>The runtime library for parsers generated by the C# target of ANTLR 4.</summary>
        <readme>assets\Readme.md</readme>
        <dependencies>
            <group targetFramework="netstandard1.5">
                <dependency id="NETStandard.Library" version="1.6.0"/>
            </group>
            <group targetFramework="netstandard2.0"/>
            <group targetFramework="netstandard2.1"/>
            <group targetFramework="net45"/>
            <group targetFramework="net5.0"/>
        </dependencies>
    </metadata>
    <files>
        <!-- Runtime Libraries -->

        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\netstandard1.5\Antlr4.Runtime.dll" target="lib\netstandard1.5"/>
        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\netstandard1.5\Antlr4.Runtime.pdb" target="lib\netstandard1.5"/>
        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\netstandard1.5\Antlr4.Runtime.xml" target="lib\netstandard1.5"/>

        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\netstandard2.0\Antlr4.Runtime.dll" target="lib\netstandard2.0"/>
        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\netstandard2.0\Antlr4.Runtime.pdb" target="lib\netstandard2.0"/>
        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\netstandard2.0\Antlr4.Runtime.xml" target="lib\netstandard2.0"/>

        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\netstandard2.1\Antlr4.Runtime.dll" target="lib\netstandard2.1"/>
        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\netstandard2.1\Antlr4.Runtime.pdb" target="lib\netstandard2.1"/>
        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\netstandard2.1\Antlr4.Runtime.xml" target="lib\netstandard2.1"/>

        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\net45\Antlr4.Runtime.dll" target="lib\net45"/>
        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\net45\Antlr4.Runtime.pdb" target="lib\net45"/>
        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\net45\Antlr4.Runtime.xml" target="lib\net45"/>

        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\net50\Antlr4.Runtime.dll" target="lib\net5.0"/>
        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\net50\Antlr4.Runtime.pdb" target="lib\net5.0"/>
        <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\net50\Antlr4.Runtime.xml" target="lib\net5.0"/>

        <!-- Source Code -->

        <file exclude="..\runtime\CSharp\Antlr4.Runtime\obj\**\*.cs" src="..\runtime\CSharp\Antlr4.Runtime\**\*.cs" target="src"/>
        <file src="..\runtime\CSharp\Antlr4.Runtime\**\$Configuration$\**\*Lexer.cs" target="src"/>

        <!-- Assets -->

        <file src="..\LICENSE.txt" target="assets\"/>
        <file src="..\images\icons\antlr.png" target="assets\"/>
        <file src="..\Readme.md" target="assets\"/>
    </files>
</package>
